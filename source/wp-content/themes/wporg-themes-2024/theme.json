{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"settings": {
		"custom": {
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--inter)",
					"lineHeight": 1.2
				}
			},
			"wporg/ratings-stars": {
				"color": {
					"fill": "var(--wp--preset--color--pomegrade-1)"
				}
			}
		}
	},
	"styles": {
		"blocks": {
			"core/post-title": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--eb-garamond)"
				},
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--blueberry-1)"
						}
					}
				}
			}
		},
		"elements": {
			"h1": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--eb-garamond)",
					"fontSize": "38px",
					"lineHeight": "1.3",
					"fontWeight": "400"
				}
			},
			"h2": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--inter)",
					"fontSize": "24px",
					"lineHeight": "1.2",
					"fontWeight": "600"
				},
				"spacing": {
					"margin": {
						"top": "var(--wp--preset--spacing--40)"
					}
				}
			}
		}
	}
}
